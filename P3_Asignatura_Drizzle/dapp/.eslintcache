\r\n    }\r\n    return (\r\n      \r\n      <>\r\n      <form onSubmit={this.mySubmitHandler}>\r\n        <h3>Calificaciones de una evaluacion</h3>\r\n        <p>Indice de evaluacion:  <input\r\n          type='text'\r\n          name='evaluacion'\r\n          pattern=\"[0-9]*\"\r\n          onChange={this.myChangeHandler}\r\n        /></p>\r\n      </form>\r\n      {table}\r\n      </>\r\n    );\r\n  }\r\n}\r\n\r\nexport default DetalleEvaluacion;","C:\\Users\\Ferna\\OneDrive\\Documentos\\Máster\\1ºcuatrimestre\\BCDA\\Prácticas\\Pfinal\\P3_Asignatura_Drizzle\\dapp\\src\\components\\Calificaciones\\EvaluacionHeader.js",[],{"ruleId":"122","replacedBy":"123"},{"ruleId":"124","replacedBy":"125"},{"ruleId":"126","severity":1,"message":"127","line":4,"column":8,"nodeType":"128","messageId":"129","endLine":4,"endColumn":27},{"ruleId":"126","severity":1,"message":"130","line":6,"column":8,"nodeType":"128","messageId":"129","endLine":6,"endColumn":27},{"ruleId":"126","severity":1,"message":"131","line":7,"column":8,"nodeType":"128","messageId":"129","endLine":7,"endColumn":20},"no-native-reassign",["132"],"no-negated-in-lhs",["133"],"no-unused-vars","'CalificacionesHead2' is defined but never used.","Identifier","unusedVar","'CalificacionesBody2' is defined but never used.","'ContractData' is assigned a value but never used.","no-global-assign","no-unsafe-negation"]